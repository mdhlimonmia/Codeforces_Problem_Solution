            "",
            "void dijkstra(int s){",
            "    for(int i = 0; i<=n; i++) dis[i] = infLL;",
            "    priority_queue<pll, vll, greater<pll>> pq;",
            "    dis[s] = 0;",
            "    pq.push({0, s});",
            "    while (!pq.empty()) {",
            "        int u = pq.top().S;",
            "        ll curD = pq.top().F;",
            "        pq.pop();",
            "        if(curD > dis[u]) continue;",
            "        for (auto v: adj[u]) {",
            "            if(dis[v.F] > curD + v.S) {",
            "                dis[v.F] = curD + v.S;",
            "                pq.push({curD + v.S, v.F});",
            "            }",
            "        }",
            "    }",
            "}"
