        "ll nCr(int n, int r) {",
        "    if (r > n) return 0;",
        "    if (r > n - r) r = n - r; // C(n, r) == C(n, n - r)",
        "    ll res = 1;",
        "    for (int i = 1; i <= r; ++i) {",
        "        res *= (n - r + i);",
        "        res /= i;",
        "    }",
        "    return res;",
        "}"
